<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ahiri.controllers.PlaylistPopUpController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0" spacing="25.0">
         <children>
            <TableView fx:id="playlistTable" prefHeight="339.0" prefWidth="600.0">
              <columns>
                <TableColumn fx:id="colName" prefWidth="349.60009765625" text="Playlist Name" />
                  <TableColumn fx:id="colCount" prefWidth="248.79998779296875" text="No of Song" />
              </columns>
            </TableView>
            <Button mnemonicParsing="false" onMouseClicked="#addSong" prefHeight="27.0" prefWidth="96.0" styleClass="btnSignIn" stylesheets="@../css/styling.css" text="Add" textFill="WHITE">
               <VBox.margin>
                  <Insets left="450.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
